	<xs:complexType name="EXTENSION-OpenStaffPositionExtension">
		<xs:annotation>
			<xs:documentation>This entity represents an open staff position that the education organization is seeking to fill.</xs:documentation>
			<xs:appinfo>
				<ann:TypeGroup>Domain Entity</ann:TypeGroup>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="OpenStaffPosition">
				<xs:sequence>
					<xs:element name="SchoolYear" type="SchoolYearType">
						<xs:annotation>
							<xs:documentation>The identifier for the school year for which the OpenStaffPosition is seeking to fill.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FullTimeEquivalency" type="EXTENSION-FullTimeEquivalency">
						<xs:annotation>
							<xs:documentation>The ratio between the hours of work expected in a position and the hours of work normally expected in a full-time position in the same setting.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OpenStaffPositionReason" type="EXTENSION-OpenStaffPositionReasonDescriptorReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The reason for the open staff position (e.g., new position, replacement, etc.).</xs:documentation>
							<xs:appinfo>
								<ann:Descriptor>OpenStaffPositionReasonDescriptor</ann:Descriptor>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="IsActive" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Indicator of whether the OpenStaffPosition is currently Active.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaxSalary" type="EXTENSION-CompensationPackageAmount" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The maximum salary or wage a person is paid before deductions (excluding differentials) but including annuities.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MinSalary" type="EXTENSION-CompensationPackageAmount" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The minimum salary or wage a person is paid before deductions (excluding differentials) but including annuities.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FundingSource" type="EXTENSION-FundingSourceDescriptorReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The funding source for open staff position.</xs:documentation>
							<xs:appinfo>
								<ann:Descriptor>FundingSourceDescriptor</ann:Descriptor>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="HighNeedAcademicSubject" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Indicator as to whether the OpenStaffPosition is filling a high-need subject area designated as a teacher shortage that may be eligible for special grants, aid or compensation.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PositionControlNumber" type="EXTENSION-PositionControlNumber" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Identifier assigned to the position to be filled.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Term" type="TermDescriptorReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The first term for the Session during the school year for which the OpenStaffPosition is seeking to fill.</xs:documentation>
							<xs:appinfo>
								<ann:Descriptor>TermDescriptor</ann:Descriptor>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="TotalBudgeted" type="EXTENSION-CompensationPackageAmount" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Including salary, the fully loaded cost budgeted for this teacher.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EXTENSION-StaffExtension">
		<xs:annotation>
			<xs:documentation>This entity represents an individual who performs specified activities for any public or private education institution or agency that provides instructional and/or support services to students or staff at the early childhood level through high school completion. For example, this includes:
1. An "employee" who performs services under the direction of the employing institution or agency is compensated for such services by the employer and is eligible for employee benefits and wage or salary tax withholdings
2. A "contractor" or "consultant" who performs services for an agreed upon fee or an employee of a management service contracted to work on site
3. A "volunteer" who performs services on a voluntary and uncompensated basis
4. An in-kind service provider
5. An independent contractor or businessperson working at a school site.</xs:documentation>
			<xs:appinfo>
				<ann:TypeGroup>Domain Entity</ann:TypeGroup>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Staff">
				<xs:sequence>
					<xs:element name="BackgroundCheck" type="EXTENSION-BackgroundCheck" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Staff background check history and disposition.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HighlyQualifiedAcademicSubject" type="AcademicSubjectDescriptorReferenceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The academic subject(s) in which the staff is deemed to be "highly qualified".</xs:documentation>
							<xs:appinfo>
								<ann:Descriptor>AcademicSubjectDescriptor</ann:Descriptor>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="ProbationCompleteDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The date the probation period ended or is scheduled to end.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Seniority" type="EXTENSION-Seniority" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Entries of job experience contributing to computations of seniority.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TeacherPreparationProgram" type="EXTENSION-TeacherPreparationProgram" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The Teacher Preparation Program(s) completed by the teacher.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Tenured" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Indicator of whether the staff member is tenured.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ApplicantReference" type="EXTENSION-ApplicantReferenceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Reference to applicant(s) represented by this staff member.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EXTENSION-StaffEducationOrganizationEmploymentAssociationExtension">
		<xs:annotation>
			<xs:documentation>This association indicates the EducationOrganization an employee, contractor, volunteer, or other service provider is formally associated with typically indicated by which organization the staff member has a services contract with or receives compensation from.</xs:documentation>
			<xs:appinfo>
				<ann:TypeGroup>Association</ann:TypeGroup>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="StaffEducationOrganizationEmploymentAssociation">
				<xs:sequence>
					<xs:element name="EmploymentEventReference" type="EXTENSION-EmploymentEventReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The associated Employment event upon hiring.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EmploymentSeparationEventReference" type="EXTENSION-EmploymentSeparationEventReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The associated separation event upon termination.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SchoolYear" type="SchoolYearType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The school year for the employment association.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EXTENSION-LeaveEventExtension">
		<xs:annotation>
			<xs:documentation>This  entity represents the recording of the dates of staff leave (e.g., personal time, vacation).</xs:documentation>
			<xs:appinfo>
				<ann:TypeGroup>Domain Entity</ann:TypeGroup>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="LeaveEvent">
				<xs:sequence>
					<xs:element name="EndDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The end date for the leave.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>===== Domain Entities =====</xs:documentation>
	</xs:annotation>
	<xs:complexType name="EXTENSION-Applicant">
		<xs:annotation>
			<xs:documentation>A person who makes a formal application for an OpenStaffPosition.</xs:documentation>
			<xs:appinfo>
				<ann:TypeGroup>Domain Entity</ann:TypeGroup>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ComplexObjectType">
				<xs:sequence>
					<xs:element name="ApplicantIdentifier" type="EXTENSION-ApplicantIdentifier">
						<xs:annotation>
							<xs:documentation>Identifier assigned to a person making formal application for an open staff position.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EducationOrganizationReference" type="EducationOrganizationReferenceType">
						<xs:annotation>
							<xs:documentation>The education organization, typically a LocalEducationAgency, associated with the applicant.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ApplicantIdentificationCode" type="ApplicantIdentificationCode" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>A unique number or alphanumeric code assigned to an applicant by a school, school system, a state, or other agency or entity.</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="Name" type="Name">
						<xs:annotation>
							<xs:documentation>Full legal name of the person.</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="Sex" type="SexType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>A person's gender.</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="BirthDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The month, day, and year on which an individual was born.</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="Address" type="Address" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The set of elements that describes an address, including the street address, city, state, and ZIP code.</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="InternationalAddress" type="InternationalAddress" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The set of elements that describes an international address.</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="Telephone" type="Telephone" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The 10-digit telephone number, including the area code, for the person.</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="ElectronicMail" type="ElectronicMail" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The numbers, letters, and symbols used to identify an electronic mail (e-mail) user within the network to which the individual or organization belongs.</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="HispanicLatinoEthnicity" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>An indication that the individual traces his or her origin or descent to Mexico, Puerto Rico, Cuba, Central, and South America, and other Spanish cultures, regardless of race. The term, "Spanish origin," can be used in addition to "Hispanic or Latino".</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="Race" type="RaceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The general racial category which most clearly reflects the individual's recognition of his or her community or with which the individual most identifies. The way this data element is listed, it must allow for multiple entries so that each individual can specify all appropriate races.</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="Citizenship" type="Citizenship" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Contains information relative to citizenship status and its associated probationary documentation.</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="Language" type="Language" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The language(s) the individual uses to communicate.</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="HighestCompletedLevelOfEducation" type="LevelOfEducationDescriptorReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The extent of formal instruction an individual has received (e.g., the highest grade in school completed or its equivalent or the highest degree received).</xs:documentation>
							<xs:appinfo>
								<ann:Descriptor>LevelOfEducationDescriptor</ann:Descriptor>								
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="YearsOfPriorProfessionalExperience" type="NumberOfYears" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The total number of years that an individual has previously held a similar professional position in one or more education institutions.</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="YearsOfPriorTeachingExperience" type="NumberOfYears" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The total number of years that an individual has previously held a teaching position in one or more education institutions.</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="LoginId" type="IdentificationCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The login ID for the user; used for security access control interface.</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="HighlyQualifiedTeacher" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>An indication of whether a teacher is classified as highly qualified for his/her assignment according to state definition. This attribute indicates the teacher is highly qualified for ALL Sections being taught.</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="Credential" type="Credential" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The legal document or authorization giving authorization to perform teaching assignment services.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BackgroundCheck" type="EXTENSION-BackgroundCheck" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Applicant background check history and disposition.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HighlyQualifiedAcademicSubject" type="AcademicSubjectDescriptorReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The academic subject(s) in which the staff is deemed to be "highly qualified".</xs:documentation>
							<xs:appinfo>
								<ann:Descriptor>AcademicSubjectDescriptor</ann:Descriptor>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="TeacherPreparationProgram" type="EXTENSION-TeacherPreparationProgram" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The Teacher Preparation Program(s) completed by the teacher.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ProspectReference" type="EXTENSION-ProspectReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Reference to previously identified prospect.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EXTENSION-Application">
		<xs:annotation>
			<xs:documentation>An application for employment or contract.</xs:documentation>
			<xs:appinfo>
				<ann:TypeGroup>Domain Entity</ann:TypeGroup>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ComplexObjectType">
				<xs:sequence>
					<xs:element name="ApplicationIdentifier" type="EXTENSION-ApplicationIdentifier">
						<xs:annotation>
							<xs:documentation>Identifier assigned to the application for an open staff position.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EducationOrganizationReference" type="EducationOrganizationReferenceType">
						<xs:annotation>
							<xs:documentation>The education organization, typically a LocalEducationAgency, associated with the application.</xs:documentation>							
						</xs:annotation>
					</xs:element>
					<xs:element name="ApplicationDate" type="xs:date">
						<xs:annotation>
							<xs:documentation>The month, day, and year the application was submitted.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ApplicationStatus" type="EXTENSION-ApplicationStatusDescriptorReferenceType">
						<xs:annotation>
							<xs:documentation>Indicates the current status of the application (e.g., received, phone screen, interview, awaiting decision, etc.).</xs:documentation>
							<xs:appinfo>
								<ann:Descriptor>ApplicationStatusDescriptor</ann:Descriptor>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="CurrentEmployee" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Indicator as to whether the applicant is a current employee of the school district.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AcademicSubject" type="AcademicSubjectDescriptorReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The academic subject for which the application is made.</xs:documentation>
							<xs:appinfo>
								<ann:Descriptor>AcademicSubjectDescriptor</ann:Descriptor>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="AcceptedDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The date of job acceptance, if offered.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ApplicationSource" type="EXTENSION-ApplicationSourceDescriptorReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Specifies the source for the application (e.g., Job fair, website, referral).</xs:documentation>
							<xs:appinfo>
								<ann:Descriptor>ApplicationSourceDescriptor</ann:Descriptor>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="FirstContactDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Date applicant was first contacted after submitting application.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HighNeedsAcademicSubject" type="AcademicSubjectDescriptorReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The high need academic subject for the application, if any.</xs:documentation>
							<xs:appinfo>
								<ann:Descriptor>AcademicSubjectDescriptor</ann:Descriptor>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="HireStatus" type="EXTENSION-HireStatusDescriptorReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Indicates the current status of the application for hire (e.g., applied, recommended, rejected, exited, offered, hired).</xs:documentation>
							<xs:appinfo>
								<ann:Descriptor>HireStatusDescriptor</ann:Descriptor>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="HiringSource" type="EXTENSION-HiringSourceDescriptorReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The source for the application (e.g.,job fair, website, referral, etc.).</xs:documentation>
							<xs:appinfo>
								<ann:Descriptor>HiringSourceDescriptor</ann:Descriptor>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="WithdrawDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The date the application was withdrawn by the applicant.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="WithdrawReason" type="EXTENSION-WithdrawReasonDescriptorReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Reason applicant withdrew application.</xs:documentation>
							<xs:appinfo>
								<ann:Descriptor>WithdrawReasonDescriptor</ann:Descriptor>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="ApplicantReference" type="EXTENSION-ApplicantReferenceType">
						<xs:annotation>
							<xs:documentation>The applicant submitting the application.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OpenStaffPositionReference" type="EXTENSION-OpenStaffPositionReferenceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The open staff position(s) associated with the application.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EXTENSION-ApplicationEvent">
		<xs:annotation>
			<xs:documentation>The life cycle events associated with an application (phone screen, review, interview, etc.).</xs:documentation>
			<xs:appinfo>
				<ann:TypeGroup>Domain Entity</ann:TypeGroup>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ComplexObjectType">
				<xs:sequence>
					<xs:element name="EventDate" type="xs:date">
						<xs:annotation>
							<xs:documentation>The date of the application event, or begin date if an interval.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EventEndDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The end date of the event, if an interval.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SequenceNumber" type="EXTENSION-SequenceNumber">
						<xs:annotation>
							<xs:documentation>The sequence number of the application events. This is used to discriminate between mutiple events of the same type in the same day.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ApplicationEventType" type="EXTENSION-ApplicationEventTypeDescriptorReferenceType">
						<xs:annotation>
							<xs:documentation>Description of application event (e.g., added to pool, phone screen, interview, sample lesson).</xs:documentation>
							<xs:appinfo>
								<ann:Descriptor>ApplicationEventTypeDescriptor</ann:Descriptor>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="ApplicationEvaluationScore" type="EXTENSION-ApplicationEvaluationScore" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Application evaluation score, if applicable.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ApplicationEventResult" type="EXTENSION-ApplicationEventResultDescriptorReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The recommendation, result or conclusion of the application event (e.g., Continue, exit, recommend for hire).</xs:documentation>
							<xs:appinfo>
								<ann:Descriptor>ApplicationEventResultDescriptor</ann:Descriptor>
							</xs:appinfo>
						</xs:annotation>
